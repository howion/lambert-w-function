{
    "compilerOptions": {
        "lib": [ "es2015" ],
        "removeComments": true,
        "noImplicitAny": false,
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": false,
        "esModuleInterop": true,
        "alwaysStrict": true,
        "declaration": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
